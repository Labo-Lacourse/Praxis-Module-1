********************;
** WIDE vers LONG **;
********************;

*Pour plusieurs blocs de variables à transposer, il faut déclarer plusieurs ARRAY;
*Retirer les indicateurs de commentaires afin de transposer simultanément nbcondam1 à nbcondam5;

DATA villeslong;  SET villeswide;
  
  ARRAY anbintervpo(1:5) nbintervpo1 - nbintervpo5 ;
  *ARRAY anbcondam(1:5) nbcondam1 - nbcondam5 ;

  DO mois = 1 to 5 ;
    nbintervpo = anbintervpo(mois);
	*nbcondam = anbcondam(mois);
    OUTPUT;
  END;

  DROP nbintervpo1 - nbintervpo5; *nbcondam1-nbcondam5;
RUN;

proc means data=villeslong mean std min max;
class mois;
var nbintervpo; *nbcondam;
run;
proc means data=villeswide mean std min max;
var nbintervpo1-nbintervpo5; *nbcondam1-nbcondam5;
run;



********************;
** LONG vers WIDE **;
********************;

PROC SORT DATA=villeslong OUT=villeslongsort ;
  BY ville ;
RUN ;

DATA villeswide ;  SET villeslongsort ;
  BY ville ;

  KEEP ville nbintervpo1 - nbintervpo5; * nbcondam1-nbcondam5;
  RETAIN nbintervpo1 - nbintervpo5; * nbcondam1-nbcondam5;

  ARRAY anbintervpo(1:5) nbintervpo1 - nbintervpo5 ;
  *ARRAY anbcondam(1:5) nbcondam1 - nbcondam5 ;

  IF first.ville THEN
  DO;
    DO i = 1 to 5 ;
      anbintervpo( i ) = . ;
	  *anbcondam( i) = . ;
    END;
  END;

  anbintervpo( mois ) = nbintervpo ;
  anbcondam( mois ) = nbcondam ;

  IF last.ville THEN OUTPUT ;

RUN;

proc means data=villeslong mean std min max;
class mois;
var nbintervpo; * nbcondam;
run;
proc means data=villeswide mean std min max;
var nbintervpo1-nbintervpo5; * nbcondam1-nbcondam5;
run;


